  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "private": true,
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "start": "ember serve",
    "start:dev": "sudo ember serve --ssl true --port 443 --environment=development",
    "test": "ember test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --check --write",
      "git add"
    ]
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
