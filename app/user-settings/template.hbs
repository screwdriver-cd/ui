{{info-message message=errorMessage type="warning" icon="exclamation-triangle"}}
{{info-message message=successMessage type="success" icon="check"}}
{{token-list
    tokens=tokens
    newToken=newToken
    onCreateToken=(action "createToken")
    onRefreshToken=(action "refreshToken")
    tokenName="User access"
    tokenScope="your account"
}}
{{user-setting
    errorMessage=errorMessage
    successMessage=successMessage
}}