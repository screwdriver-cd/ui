{{#if this.pipeline}}
  <div class="workflow">
    <WorkflowGraphD3
      @pipeline={{this.pipeline}}
      @workflowGraph={{this.workflowGraph}}
      @startFrom={{this.startFrom}}
      @graphClicked={{action "graphClicked"}}
    >
      <WorkflowTooltip
        @tooltipData={{this.tooltipData}}
        @showTooltip={{this.showTooltip}}
        @showTooltipPosition={{this.showTooltipPosition}}
      />
    </WorkflowGraphD3>
  </div>
{{else}}
  <div class="pipeline-vis empty">
    {{svg-jar "pipeline-vis-display"}}
  </div>
{{/if}}