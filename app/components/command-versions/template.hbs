<h4>
  Versions:
</h4>
<ul>
  {{#each this.commands.commandData as |c|}}
    <li>
      <LinkTo
        @route="commands.detail"
        @models={{array c.namespace c.name c.version}}
      >
        <span class="version">
          {{c.version}}{{#if c.tag}}
            - {{c.tag}}
          {{/if}}
        </span>
      </LinkTo>{{#if c.lastUpdated}}
        {{c.lastUpdated}}
      {{/if}}
    </li>
  {{/each}}
</ul>