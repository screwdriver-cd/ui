{{#if session.isAuthenticated}}
  {{pipeline-nav pipeline=pipeline}}
{{/if}}

{{pipeline-secret-settings
  secrets=secrets
  onCreateSecret=(action "createSecret")
  pipeline=pipeline
  errorMessage=errorMessage
}}

{{token-list
    tokens=pipelinetokens
    newToken=newToken
    pipelineId=pipelineId
    onCreateToken=(action "createPipelineToken")
    onRefreshToken=(action "refreshPipelineToken")
    tokenName="Pipeline"
    tokenScope="this pipeline"
}}

{{outlet}}
